{
    "name": "read-bigint",
    "version": "0.0.1",
    "description": "ðŸ‘“ðŸ’¯ Read Unsigned/Signed Big/Little-Endian BigInt from Buffer",
    "keywords": [
        "bigint",
        "buffer",
        "read",
        "readbigint",
        "signed",
        "unsigned"
    ],
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "license": "MIT",
    "homepage": "https://github.com/oBusk/read-bigint/tree/master/packages/read-bigint#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oBusk/read-bigint.git"
    },
    "bugs": {
        "url": "https://github.com/oBusk/read-bigint/issues"
    },
    "files": [
        "dist/",
        "dist.module/",
        "!**/*.spec.*"
    ],
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist.module/index.js",
    "sideEffects": false,
    "bundlewatch": {
        "files": [
            {
                "path": "dist?(.module)/*.js",
                "maxSize": "500B"
            }
        ]
    },
    "scripts": {
        "bundlewatch": "bundlewatch",
        "lint": "eslint **/*.{js,jsx,ts,tsx,json} --ignore-path ../../.gitignore",
        "prettier": "prettier **/*.{md,yml,yaml} --ignore-path ../../.gitignore --check",
        "build": "rimraf dist/ dist.module/ && tsc && tsc -P tsconfig.module.json",
        "test": "jest",
        "test-coverage": "jest --coverage",
        "prepare": "npm run build"
    },
    "dependencies": {
        "read-bigint-64-be": "^0.0.1",
        "read-bigint-64-le": "^0.0.1",
        "read-biguint-64-be": "^0.0.1",
        "read-biguint-64-le": "^0.0.1"
    },
    "devDependencies": {
        "@types/jest": "^25.1.3",
        "bundlewatch": "^0.2.5",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "prettier": "^1.19.1",
        "ts-jest": "^25.2.1",
        "typescript": "^3.8.3"
    }
}
